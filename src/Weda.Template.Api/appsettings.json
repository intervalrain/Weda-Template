{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "DATABASE_PROVIDER_PLACEHOLDER",
    "ConnectionString": "DATABASE_CONNECTION_PLACEHOLDER",
    "DatabaseName": "WedaTemplate"
  },
  "Authentication": {
    "Enabled": true
  },
  "Nats": {
    "Url": "nats://localhost:4222",
    "Name": "weda-template",
    "Connections": {
      "default": {
        "Url": "nats://localhost:4222"
      },
      "bus": {
        "Url": "nats://localhost:4223"
      }
    }
  },
  "JwtSettings": {
    "Secret": "a-very-super-secret-key-that-is-long-enough",
    "TokenExpirationInMinutes": 0,
    "Issuer": "WedaTemplate",
    "Audience": "WedaTemplate"
  },
  "EmailSettings": {
    "EnableEmailNotifications": false,
    "DefaultFromEmail": "",
    "SmtpSettings": {
      "Server": "",
      "Port": 0,
      "Username": "",
      "Password": ""
    }
  }
}