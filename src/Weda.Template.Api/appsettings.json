{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
#if (db == "sqlite")
    "Provider": "Sqlite",
    "ConnectionString": "Data Source=Weda.Template.sqlite",
#elseif (db == "postgres")
    "Provider": "PostgreSql",
    "ConnectionString": "Host=localhost;Database=weda_template;Username=postgres;Password=postgres",
#elseif (db == "mongo")
    "Provider": "MongoDb",
    "ConnectionString": "mongodb://localhost:27017",
#elseif (db == "none")
    "Provider": "InMemory",
    "ConnectionString": "",
#endif
    "DatabaseName": "WedaTemplate"
  },
  "Authentication": {
    "Enabled": true
  },
  "Nats": {
    "Url": "nats://localhost:4222",
    "Name": "weda-template",
    "Connections": {
      "default": {
        "Url": "nats://localhost:4222"
      },
      "bus": {
        "Url": "nats://localhost:4223"
      }
    }
  },
  "JwtSettings": {
    "Secret": "a-very-super-secret-key-that-is-long-enough",
    "TokenExpirationInMinutes": 0,
    "Issuer": "WedaTemplate",
    "Audience": "WedaTemplate"
  },
  "EmailSettings": {
    "EnableEmailNotifications": false,
    "DefaultFromEmail": "",
    "SmtpSettings": {
      "Server": "",
      "Port": 0,
      "Username": "",
      "Password": ""
    }
  }
}